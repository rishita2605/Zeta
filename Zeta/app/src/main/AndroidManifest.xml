<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zeta">

    <uses-permission android:name="android.permission.INTERNETTE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zeta">
        <activity android:name=".Services"/>
        <activity android:name=".Location" />
        <activity android:name=".Login" />
        <activity android:name=".Signup" />
        <activity android:name=".Bedvaccancy"/>
        <activity android:name=".Childcare"/>
        <activity android:name=".OxygenCylinders"/>
        <activity android:name=".MentalHealth"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>